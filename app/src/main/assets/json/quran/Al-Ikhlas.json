[{"chapter_id":112,"juz_number":30,"text_madani":"قُلْ هُوَ اللَّهُ أَحَدٌ","translations":[{"language_name":"english","text":"Say, \"He is Allah, [who is] One,"},{"language_name":"indonesian","text":"Katakanlah (Muhammad), “Dialah Allah, Yang Maha Esa."}],"verse_number":1},{"chapter_id":112,"juz_number":30,"text_madani":"اللَّهُ الصَّمَدُ","translations":[{"language_name":"indonesian","text":"Allah tempat meminta segala sesuatu."},{"language_name":"english","text":"Allah, the Eternal Refuge."}],"verse_number":2},{"chapter_id":112,"juz_number":30,"text_madani":"لَمْ يَلِدْ وَلَمْ يُولَدْ","translations":[{"language_name":"english","text":"He neither begets nor is born,"},{"language_name":"indonesian","text":"(Allah) tidak beranak dan tidak pula diperanakkan."}],"verse_number":3},{"chapter_id":112,"juz_number":30,"text_madani":"وَلَمْ يَكُن لَّهُ كُفُوًا أَحَدٌ","translations":[{"language_name":"english","text":"Nor is there to Him any equivalent.\""},{"language_name":"indonesian","text":"Dan tidak ada sesuatu yang setara dengan Dia.”"}],"verse_number":4}]