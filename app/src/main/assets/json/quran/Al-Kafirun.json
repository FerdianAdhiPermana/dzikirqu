[{"chapter_id":109,"juz_number":30,"text_madani":"قُلْ يَا أَيُّهَا الْكَافِرُونَ","translations":[{"language_name":"english","text":"Say, \"O disbelievers,"},{"language_name":"indonesian","text":"Katakanlah (Muhammad), “Wahai orang-orang kafir!"}],"verse_number":1},{"chapter_id":109,"juz_number":30,"text_madani":"لَا أَعْبُدُ مَا تَعْبُدُونَ","translations":[{"language_name":"english","text":"I do not worship what you worship."},{"language_name":"indonesian","text":"aku tidak akan menyembah apa yang kamu sembah,"}],"verse_number":2},{"chapter_id":109,"juz_number":30,"text_madani":"وَلَا أَنتُمْ عَابِدُونَ مَا أَعْبُدُ","translations":[{"language_name":"english","text":"Nor are you worshippers of what I worship."},{"language_name":"indonesian","text":"dan kamu bukan penyembah apa yang aku sembah,"}],"verse_number":3},{"chapter_id":109,"juz_number":30,"text_madani":"وَلَا أَنَا عَابِدٌ مَّا عَبَدتُّمْ","translations":[{"language_name":"english","text":"Nor will I be a worshipper of what you worship."},{"language_name":"indonesian","text":"dan aku tidak pernah menjadi penyembah apa yang kamu sembah,"}],"verse_number":4},{"chapter_id":109,"juz_number":30,"text_madani":"وَلَا أَنتُمْ عَابِدُونَ مَا أَعْبُدُ","translations":[{"language_name":"english","text":"Nor will you be worshippers of what I worship."},{"language_name":"indonesian","text":"dan kamu tidak pernah (pula) menjadi penyembah apa yang aku sembah."}],"verse_number":5},{"chapter_id":109,"juz_number":30,"text_madani":"لَكُمْ دِينُكُمْ وَلِيَ دِينِ","translations":[{"language_name":"english","text":"For you is your religion, and for me is my religion.\""},{"language_name":"indonesian","text":"Untukmu agamamu, dan untukku agamaku.”"}],"verse_number":6}]